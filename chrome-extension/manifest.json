{
  "name": "__MSG_extension_name__",
  "version": "0.7.2",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_extension_description__",
  "icons": {
    "128": "icon/icon.png"
  },
  "permissions": [
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [{
      "all_frames": true,
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }],
  "minimum_chrome_version": "46.0"
}
